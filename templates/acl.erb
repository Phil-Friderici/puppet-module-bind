acl "<%= @name %>" {
<% if not @entries.nil? and @entries.empty? == false -%>
<%   @entries.each do |entry| -%>
<%     if /^!?(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])(\/([0-9]|[1-2][0-9]|3[0-2]))$/.match(entry) -%>
  <%= entry %>;
<%     else -%>
<%       if entry[0..0] == '!' -%>
  !"<%= entry[1..-1] %>";
<%       else -%>
  "<%= entry %>";
<%       end -%>
<%     end -%>
<%   end -%>
<% end -%>

<% if not @keys.nil? -%>
<%   @keys.each do |key| -%>
  key "<%= key %>";
<%   end -%>
<% end -%>
};
